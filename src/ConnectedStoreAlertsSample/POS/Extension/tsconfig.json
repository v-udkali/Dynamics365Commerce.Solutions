{
  "exclude": [
	"ConnectedStoreAlertsSample"
  ],
  "compilerOptions": {
    "module": "system",
    "alwaysStrict": true,
    "declaration": true,
    "lib": [ "dom", "es5", "scripthost", "es2015.promise" ],
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "removeComments": true,
    "sourceMap": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "target": "es5",
    "typeRoots": [ "./typings" ],
    "rootDir": "./",
    "mapRoot": "./",
    "sourceRoot": "./"
  }
}